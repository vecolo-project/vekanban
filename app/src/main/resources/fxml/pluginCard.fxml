<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="250.0" spacing="15.0" style="-fx-border-radius: 5; -fx-background-color: -darkGray; -fx-border-color: -gray;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.vecolo.vekanban.app.controllers.PluginCardController">
    <children>
          <Label fx:id="pluginName" alignment="BASELINE_LEFT" styleClass="h4" text="pluginName" />
      <TextArea fx:id="pluginDescription" editable="false" style="-fx-border-color: #fefefe;" styleClass="bord" wrapText="true" VBox.vgrow="ALWAYS" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
