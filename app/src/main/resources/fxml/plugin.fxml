<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="657.0" prefWidth="600.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.vecolo.vekanban.app.controllers.PluginController">
    <children>
      <HBox spacing="20.0">
         <children>
              <Label fx:id="pluginName" styleClass="h2" text="Plugin name" wrapText="true" />
            <Label fx:id="pluginVersion" layoutX="10.0" layoutY="10.0" styleClass="h2" text="Plugin version" wrapText="true" />
         </children>
      </HBox>
      <Label fx:id="pluginDescription" alignment="TOP_LEFT" prefHeight="100.0" prefWidth="581.0" style="-fx-border-color: #fefefe;" styleClass="h4" text="Label" wrapText="true">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Label>
      <VBox fx:id="textFieldBox" spacing="10.0">
         <children>
            <Label fx:id="textFieldPrompt" styleClass="h4" text="TextField prompt :" wrapText="true" />
            <TextField fx:id="textField" style="-fx-background-color: -darkGray;" styleClass="h5" />
         </children>
      </VBox>
      <VBox fx:id="textAreaBox" layoutX="20.0" layoutY="136.0" spacing="10.0">
         <children>
            <Label fx:id="textAreaPrompt" styleClass="h4" text="TextArea prompt :" wrapText="true" />
            <TextArea fx:id="textArea" prefHeight="130.0" prefWidth="580.0" style="-fx-border-color: #fefefe;" styleClass="h5" wrapText="true" />
         </children>
      </VBox>
      <VBox fx:id="existingBoardBox" spacing="10.0">
         <children>
            <Label fx:id="existingBoardPrompt" styleClass="h4" text="Existing board prompt :" wrapText="true" />
            <ComboBox fx:id="existingBoardComboBox" prefHeight="23.0" prefWidth="180.0" styleClass="h5" />
         </children>
      </VBox>
      <VBox fx:id="fileNameBox" spacing="10.0">
         <children>
            <Label fx:id="filenamePrompt" styleClass="h4" text="Filename prompt :" wrapText="true" />
            <TextField fx:id="filenameField" style="-fx-background-color: -darkGray;" styleClass="h5" />
         </children>
      </VBox>
      <Button fx:id="runButton" mnemonicParsing="false" onAction="#onRun" style="-fx-background-color: #36A024;" styleClass="h4" text="Lancer" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
